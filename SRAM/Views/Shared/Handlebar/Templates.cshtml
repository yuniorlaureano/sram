<script type="text/html" id="pending-summary">
    {{#each summary}}
    <tr>
        <td>{{Name}}</td>
        <td class="td-count-summary">{{Count}}</td>
    </tr>
    {{/each}}
</script>

<script type="text/html" id="subscriber-credit">
    {{#each credit}}
    <tr>
        <th>{{ClaimNumber}}</th>
        <th>{{Book}}</th>
        <th>{{ClientComment}}</th>
        <th>{{InfoDescription}}</th>
    </tr>
    {{/each}}
</script>

<script type="text/html" id="subscriber-claim">
    {{#each claim}}
    <tr>
        <th>{{ClaimNumber}}</th>
        <th>{{Book}}</th>
        <th>{{ClaimDescription}}</th>
        <th>{{ClientComment}}</th>
        <th>{{CanvCode}}</th>
    </tr>
    {{/each}}
</script>